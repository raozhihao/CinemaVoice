<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/>
  </startup>
  <appSettings>
    <!--
    动态加载的程序集,获取所有电影列表,参数(程序集名称,对象名称)
    -->
    <add key="MovieList" value="程序集名称,程序集名称.对象名称" />
    <!--api存放位置-->
    <add key="apiPath" value="API.dll"/>
    
    <!--动态加载的程序集,以模糊查询获取当前搜索关键字的电影参数(程序集名称,对象名称)-->
    <add key="Search" value="MtimeShowList,MtimeShowList.GetMovieNameList"/>
    
    <add key="ClientSettingsProvider.ServiceUri" value="" />
    
    <!--百度api相关-->
    <add key="API_KEY" value="你的API_KEY" />
    <add key="SECRET_KEY" value="你的SECRET_KEY" />
  </appSettings>
</configuration>