<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <!--动态加载的程序集,获取所有电影列表,参数(程序集名称,对象名称)-->
    <add key="MovieList" value="MtimeShowList,MtimeShowList.Mtime"/>
    <!--动态加载的程序集,以模糊查询获取当前搜索关键字的电影参数(程序集名称,对象名称)-->
    <add key="Search" value="MtimeShowList,MtimeShowList.GetMovieNameList"/>
  </appSettings>
</configuration>